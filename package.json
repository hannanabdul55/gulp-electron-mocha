{
  "name": "gulp-electron-mocha",
  "version": "1.1.0",
  "description": "Gulp plugin to run Mocha tests in Electron",
  "author": "nodaguti",
  "license": "MIT",
  "main": "./lib/index.js",
  "repository": "nodaguti/gulp-electron-mocha",
  "scripts": {
    "build": "babel src --out-dir lib --source-maps",
    "prepublish": "npm run --if-present build",
    "test": "npm-run-all lint test:unit test:electronMocha",
    "lint": "eslint src test",
    "test:unit": "mocha test/unit",
    "test:electronMocha": "mocha test/electronMocha/test.js --timeout 12000"
  },
  "keywords": [
    "gulpplugin",
    "mocha",
    "electron"
  ],
  "dependencies": {
    "electron-mocha": "^0.7.0",
    "electron-prebuilt": "^0.36.0",
    "fs-promise": "^0.3.1",
    "gulp-util": "^3.0.7",
    "object-assign": "^4.0.1",
    "object-to-spawn-args": "^0.1.2",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "^6.0.4",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-espower": "^2.2.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "gulp": "^3.9.0",
    "npm-run-all": "^2.1.2",
    "power-assert": "^1.2.0"
  }
}
